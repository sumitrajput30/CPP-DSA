#include<bits/stdc++.h>
using namespace std;
int main()
{
    int r=4;
    int c=r;
    vector<vector<int>>v;
    int num;
    for(int i=0;i<r;i++)
    {
        vector<int>node;
        for(int j=0;j<c;j++)
        {
            cout<<"enter node\n";
         cin>>num;
        node.push_back(num);



        }
        v.push_back(node);

    }
    cout<<"results are\n";
    for(int i=0;i<v.size();i++)
    {
        for(int j=0;j<v[i].size();j++)
        {
            cout<<v[i] [j]<<" ";
        }
        cout<<"\n";
    }
}
